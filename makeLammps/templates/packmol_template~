tolerance 2.0
filetype pdb
output solvated_poly.pdb

structure waterfile
  number N_water
  resnumbers 0
  inside box -Len -Len -Len Len Len Len
end structure

structure polyfile
  number N_poly
  resnumbers 2
  centerofmass
  #fixed 0. 0. 0. 0. 0. 0.
  inside box -Len -Len -Len Len Len Len
end structure
 
